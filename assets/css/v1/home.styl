#qz-home
    .v-navigation-drawer-custom
        background: #F5F5F5;
    
    .qz-user
        height: 64px
        background-color: #FBFBFB 
        .v-toolbar__content
            padding: 8px 22px 8px 18px
        .qz-user-avatar
            width: 48px 
            height: 48px
            float: left
            img
                width: 100%
                height: 100%
                background-color: #aaaaaa
                border-radius: 50%
        .qz-user-meta
            width: 160px
            margin-left: 8px
        .qz-user-username
            font-size: 14px;
            color: #404040;
        .v-progress-linear
            margin: 2px 0 3px 0
            .v-progress-linear__background
                background: #E9E9E9
        .qz-user-amount, .qz-user-amount-upgrade
            font-size: 12px;
            color: #5A5A5A;
            letter-spacing: 0.5px;
            float: left
        .qz-user-amount-upgrade
            color: #2EC17C
            float: right

    .qz-left
        .v-list__tile
            padding: 0 18px
            .v-list__tile__action
                min-width: 24px
            .v-icon 
                font-size: 18px
            .v-list__tile__title
                font-size: 16px;
                font-weight: middle
                color: #404040;
                letter-spacing: 0.67px;
            &.v-list__tile--active
                background: #E9E9E9
                .v-icon, .v-list__tile__title
                    color: #2EC17C;
    .qz-top
        .v-toolbar__content
            background: #FBFBFB
            padding: 0 32px
            border-bottom: 1px solid #D9D9D9;
        .v-toolbar__title
            width: 70px
            text-align: center
            margin-left: 0
            margin-right: 48px
            color: #5A5A5A;
            border-bottom: 3px solid transparent
            a
                line-height: 22px
                font-size: 16px;
                color: #5A5A5A;
                text-decoration: none
            &.active
                border-bottom: 3px solid #2EC17C
                a
                    color: #2EC17C;
    .v-button-custom
        margin: 0
        padding: 0
        height: 32px
        border: 1px solid #BFBFBF
        border-radius: 5px
        &.qz-upload
            width: 84px
            min-width: 84px
            border: none 
            box-shadow: 0 2px 4px 0 rgba(223,223,223,0.50);
            .v-btn__content span 
                margin-left: 8px
        &.qz-new-folder
            width: 112px
            .v-btn__content span 
                margin-left: 4px
        .v-btn__content
            font-size: 14px 
        &.v-button-primary-custom .v-btn__content
            color: #fff
    .qz-toolbar-layout
        overflow: hidden
        padding: 16px 18px 10px 18px

    .m-r-16
        margin-right: 16px
    .v-button-group
        .v-button-custom
            border-radius: 0
            min-width: 64px
            width: 64px
            border-left: none
            .v-btn__content .v-icon 
                margin-right: 4px
            &:first-child
                border-left: 1px solid #BFBFBF
                border-radius: 5px 0 0 5px
            &:last-child
                border-radius: 0 5px 5px 0
    .v-icon-button-custom
        width: 30px
        min-width: 30px
        margin-left: 8px
        border: none !important
        &.v-btn--active:before, &.v-btn:hover:before, &.v-btn:focus:before 
            background-color: transparent
        
    .v-input-text-custom
        max-width: 224px
        height: 32px
        background: #F5F5F5;
        border: 1px solid #D9D9D9;
        border-radius: 5px;
        font-size: 14px;
        color: #919191;
        margin-right: 6px
        .v-input__control
            min-height: 32px
        .v-text-field__slot
            input
                max-height: inherit
                font-size: 14px
                color: #404040
                padding: 0
        .v-input__slot
            background: transparent

.qz-breadcrumbs
    padding: 0 18px
    a
        color: #2EC17C !important;
        &:hover
            text-decoration: underline
    .qz-breadcrumbs-back
        margin: 0
        padding: 0
        width: 63px 
        min-width: 63px
        .v-btn__content
            font-size: 12px
    .qz-breadcrumbs-back-divider
        font-size: 14px;
        padding: 0 4px
        align-items: center;
        display: -webkit-inline-box;
        display: -ms-inline-flexbox;
        display: inline-flex;
        .v-icon
            font-size: 16px;
            width: 5px
            color: #BBBBBB;
    .qz-breadcrumbs-back-divider-icon
        font-size: 10px;
        color: #bbb;
        align-items: center;
        display: -webkit-inline-box;
        display: -ms-inline-flexbox;
        display: inline-flex;
        margin: 0 4px
    .qz-breadcrumbs-back-divider-prefix
        width: 15px
        text-align: center
        align-items: center;
        display: -webkit-inline-box;
        display: -ms-inline-flexbox;
        display: inline-flex;
        font-size: 14px;
        letter-spacing: 0.58px;
        color: #404040;
    .v-breadcrumbs
        padding: 0
        li .v-icon
            width: 5px
            color: #BBBBBB;
    .v-breadcrumbs__item
        font-size: 12px;
        color: #404040;
        letter-spacing: 0.5px;
    .v-breadcrumbs__item--disabled
        color: #404040 !important
    li:nth-child(even)
        padding: 0 4px 
        
#qz-table
    table.v-table thead td:first-child, table.v-table tbody td:first-child, table.v-table thead th:first-child, table.v-table tbody th:first-child 
        padding: 0
    thead th:first-child, tbody td:first-child
        width: 30px
        .v-input__slot
            margin-bottom: 4px
        .v-input--selection-controls__input
            margin-left: auto;
            margin-right: auto;
            .v-icon
                margin: 0 auto
                color: #BFBFBF
                font-size: 16px
    thead th, tbody td
        padding: 0 8px 
    thead
        font-size: 12px
        color: #5A5A5A
        background: #F7F7F7;
        height: 32px
        tr 
            height: 32px !important
            border-bottom: none !important
            th.active
                background: #E9E9E9;
                .v-icon 
                    color: #2EC17C;
        .v-datatable__progress
            display: none
    tbody
        tr 
            background: #FBFBFB;
            &:hover, &.v-tr-active
                background: #E9E9E9;
                border-color: #D9D9D9;
            &:not(:last-child)
                border-color: #FBFBFB;
        .qz-td-file
            cursor: pointer
            .qz-filename
                margin-left: 8.8px 
                width: 440px
                text-overflow: ellipsis
                white-space: nowrap
                overflow: hidden
                line-height: 26px
                color: #404040;
                text-decoration: none
            .qz-td-file-btn-group
                display: none
                width: 40px
                .v-icon
                    float: right
                .v-icon:last-child 
                    margin-left: 10px
            &:hover 
                .qz-filename
                    width: 400px
                    &.href-link
                        text-decoration: underline
                .qz-td-file-btn-group
                    display: inherit
        .qz-rename
            width: 120px 
            height: 25px
            margin-left: 8.8px
            flex: none
            .v-input__control, input
                min-height: 25px
            .v-input__control
                background: #FFFFFF;
                border: 1px solid #A5D6A7;
                box-shadow: 0 0 4px 0 #2EC17C;
                border-radius: 2px;
            .v-input__slot
                padding: 0 8px
            input
                padding: 0
                font-size: 14px;
                color: #404040;
            ::selection
                background: #C8E6C9;
        .qz-rename-btn
            margin: 2px 0 0 7px
            padding: 0
            width: 23px
            min-width: 23px
            height: 23px
            background: #FFFFFF;
            border: 1px solid #A5D6A7;
            border-radius: 2px;
            .v-icon
                font-size: 11px
        .qz-rename-btn-submit .v-icon
            color: #2EC17C;
        .qz-rename-btn-cancel .v-icon
            color: #D32F2F;
                        

.qz-no-data
    min-height: 700px
    .v-icon 
        margin-right: -28px
        margin-bottom: 18px
    .qz-title
        font-weight: bold
        font-size: 14px;
        color: #757575;
        letter-spacing: 0.58px;
        margin-bottom: 4px
    .qz-subtitle
        font-size: 14px;
        color: #757575;
        letter-spacing: 0.58px;

.qz-file-menu
    background: #FFFFFF;
    border: 1px solid #E9E9E9;
    box-shadow: 2px 2px 4px 0 rgba(205,205,205,0.50);
    border-radius: 3px;
    .v-list
        padding: 3px 0
    .v-list__tile
        height: 28px
        padding: 3px 18px;
        .v-list__tile__title
            font-size: 14px;
            color: #404040;
            text-align: center;
            line-height: 28px;
    
.qz-dialog
    .qz-btn
        min-width: 65px
    .v-card__title
        padding: 16px 0
        color: rgba(0,0,0,0.85);
        font-weight: bold
        font-size: 16px;
        line-height: 24px;
        .qz-dialog-close
            width: 24px
            height: 24px
            line-height: 24px;
            margin: 0
            .v-icon
                color: rgba(0,0,0,0.25);
                font-size: 12px
    .qz-btn-create-dir
        width: 122px
        .v-btn__content
            letter-spacing: 1px;
    .qz-dirname
        margin: 12px 32px 18px 32px
        border: 1px solid #A3DAC0;
        box-shadow: 0 0 4px 0 #2EC17C;
        border-radius: 4px;
    .v-card__actions
        padding: 11px 31px 9px 31px

.qz-dirlist
    padding: 0
    height: 320px;
    overflow-y: auto;
    overflow-x: hidden;
    .v-list 
        width: 100%
    .v-list__tile
        padding: 0 16px 
        font-size: 12px;
        color: #5A5A5A;
    .qz-list-tile
        .v-list__tile--link:hover
            background: transparent
    .qz-list-tile:hover
        background: #EFF9F4
    .qz-list-tile-active
        background: #B8EBD3 !important;
    .v-list__tile--avatar
        height: 32px
    .qz-dirlist-expand
        min-width: 0
        padding: 0 8px
        .v-icon
            width: 12px
            color: #979797;
            font-size: 14px
    .qz-dirlist-avatar
        min-width: 13px
        margin-right: 8px 
    .v-list__tile__content
        max-width: 480px

.qz-thumb-item
    width: 128px
    max-width: 128px
    height: 128px
    margin: 55px 0 0 55px
    border: 1px solid transparent;
    &:hover 
        background: #F5F5F5;
    &.qz-active:hover
        border-color: #2EC17C;
    &.qz-active
        background: #F5F5F5;
    .v-card
        width: 128px
        height: 128px
        background: transparent
        position: relative
        .v-icon 
            width: 100%;
            margin: 27px auto 23px
        .qz-filename
            height: 20px 
            line-height: 20px
            width: 100px
            font-size: 14px;
            color: #404040;
            text-decoration: none
            letter-spacing: 0.93px;
            text-align: center
            text-overflow: ellipsis
            white-space: nowrap
            overflow: hidden
            margin-left: auto 
            margin-right: auto 
            user-select: none
        .qz-rename
            margin: 0 auto !important;
            width: 113px !important;
        .qz-icon-check
            position: absolute
            width: 15px 
            height: 15px
            top: 8px 
            left: 8px
            margin: 0
            display: none
            &.qz-active
                display: block !important
        &:hover
            &.qz-active
                border-color: #2EC17C;
            .qz-icon-check
                display: block


.qz-listtype-thumb
    tbody 
        display: flex
        flex-wrap: wrap
        flex-direction: row
        padding-right: 55px 
        padding-bottom: 55px
        tr 
            width: 100%;
            min-height: 700px;
            td[colspan] 
                width: 100% !important;
                min-height: 700px;
                display: flex;

